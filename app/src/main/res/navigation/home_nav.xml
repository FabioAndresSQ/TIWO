<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_nav"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.faesfa.tiwo.HomeFragments.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_presetsFragment"
            app:destination="@id/presetsFragment" />
    </fragment>
    <fragment
        android:id="@+id/presetsFragment"
        android:name="com.faesfa.tiwo.HomeFragments.PresetsFragment"
        android:label="fragment_presets"
        tools:layout="@layout/fragment_presets" >
        <action
            android:id="@+id/action_presetsFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
</navigation>